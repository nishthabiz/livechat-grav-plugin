{
    "name": "nishtha/nreach-livechat",
    "type": "grav-plugin",
    "description": "Simple plugin to install the nReach Livechat widget onto any Grav site",
    "keywords": [
        "livechat",
        "livechat widget",
        "nreach"
    ],
    "homepage": "https://github.com/nishthabiz/livechat-grav-plugin/",
    "license": "MIT",
    "authors": [
        {
            "name": "Nishthabiz",
            "email": "reach@nreach.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Grav\\Plugin\\NreachLivechat\\": "classes/"
        },
        "classmap": [
            "nreach-livechat.php"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        }
    }
}
